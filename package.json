{
  "name": "winsight",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "cypress": "cypress open",
    "test:eslint": "echo \"Running eslint check\" && eslint . --max-warnings=0",
    "test:eslint-fix": "echo \"Running eslint check\" && eslint . --fix --max-warnings=0",
    "test:tsc": "echo \"Running typescript check\" && tsc",
    "test:e2e": "concurrently \"npm run web\" \"cypress run --browser electron\"",
    "test:coverage": "nyc report --reporter=text-summary",
    "tests": "npm run test:eslint && npm run test:tsc",
    "coverage:print-env": "npx print-env GITHUB",
    "coverage:set-gh-status": "npx set-gh-status --check-against-readme",
    "coverage:update-badge": "npx update-badge"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo/webpack-config": "^19.0.0",
    "@mui/material": "^5.14.13",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-navigation/native": "^6.1.8",
    "@react-navigation/native-stack": "^6.9.14",
    "expo": "~49.0.11",
    "expo-status-bar": "~1.7.1",
    "expo-font": "~11.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.5",
    "react-native-safe-area-context": "4.7.2",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-screens": "~3.22.0",
    "react-native-vector-icons": "^10.0.0",
    "react-native-web": "~0.19.6",
    "react-native-webview": "13.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@bahmutov/print-env": "^2.1.2",
    "@cypress/code-coverage": "^3.12.4",
    "@types/react": "~18.2.14",
    "@types/react-native-vector-icons": "^6.4.15",
    "babel-plugin-istanbul": "^6.1.1",
    "check-code-coverage": "^1.10.5",
    "concurrently": "^8.2.1",
    "cypress": "^13.3.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-universe": "^12.0.0",
    "eslint-plugin-cyclomatic-complexity": "^2.0.11",
    "eslint-plugin-etc": "^2.0.3",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "expo-updates": "^0.18.15",
    "nyc": "^15.1.0",
    "prettier": "^3.0.3",
    "typescript": "^5.1.3"
  },
  "private": true
}
